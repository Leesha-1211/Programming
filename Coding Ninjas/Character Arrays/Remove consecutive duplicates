#include<string>

void removeConsecutiveDuplicates(char input[]) {
    int len=strlen(input);
    int k=0;
    char arr[100];
     arr[k]=input[0];
    k++;
    for(int i=0;i<len;i++){
        if(arr[k]==input[i]){
               i++;
        } else {
          arr[k] = input[i];
          k++;
        }
    }
    }



