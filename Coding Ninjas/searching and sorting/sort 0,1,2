
void sort012(int *arr,int n){
    int low=0,high=n-1,i=0;
    
    while(i<=high){
        if(arr[i]==0){
            swap(arr[i],arr[low]);
            low++;
            i++;
        }else if(arr[i]==2){
            swap(arr[i],arr[high]);
            high--;
        }else{
            i++;
        }
    }
}
