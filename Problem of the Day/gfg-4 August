class Solution{
public:
    void Reverse(stack<int> &St){
        
        vector<int>lee;
        while(!St.empty()){
            
            lee.push_back(St.top());
            St.pop();
            
            
        }
      
      for(int i=0;i<lee.size();i++){
          St.push(lee[i]);
      }
        
    }
};
